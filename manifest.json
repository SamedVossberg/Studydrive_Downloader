{
    "manifest_version": 3,
    "name": "Studydrive Skipper",
    "version": "1.0",
    "description": "Download all Studydrive files.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }